# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Database
DATABASE_PATH=./data/aima.db

# Logging
LOG_LEVEL=INFO

# AIMA URLs (optional - defaults provided)
# AIMA_LOGIN_URL=https://services.aima.gov.pt/RAR/login.php
# AIMA_CHECK_URL=https://services.aima.gov.pt/RAR/login_check3.php

# SSL Verification (set to false if AIMA has certificate issues)
# VERIFY_SSL=false

# HTTP/HTTPS Proxy (optional - uncomment and configure to enable)
# Format: http://username:password@proxy.example.com:port
# PROXY_URL=http://user:pass@proxy.example.com:8080
